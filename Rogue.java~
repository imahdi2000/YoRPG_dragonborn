public class Rogue extends Character{

    private String name;
    
    public Rogue(){
	name = "unknown";
	life = 100;
	strength = 130;
	defense = 5;
	attack = 0.4;
    }

    public Rogue(String n){
	this();//calls default constructor
	name = n;
    }
    
}
