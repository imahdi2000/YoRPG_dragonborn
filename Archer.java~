public class Archer extends Character{

    private String name;
    
    public Archer(){
	name = "unknown";
	life = 125;
	strength = 100;
	defense = 20;
	attack = 0.5;
    }

    public Archer(String n){
	this();//calls default constructor
	name = n;
    }
    
}
