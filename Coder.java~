public class Coder extends Character{

    private String name;
    
    public Coder(){
	name = "unknown";
	life = 999;
	strength = 999;
	defense = 999;
	attack = 999;
    }

    public Coder(String n){
	this();//calls default constructor
	name = n;
    }
    
}
